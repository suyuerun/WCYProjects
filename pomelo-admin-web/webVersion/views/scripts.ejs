<html>
<head>
    <title></title>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>

    <script src="/js/scripts.js"></script>
    <script src="/js/vue.min.js"></script>
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <script src="/js/jquery-3.2.1.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>

</head>
<body >
<%include common%>
<div id="serverList">
    <div  class="form-group" style="width: 80%;margin-left: 3em">
        <label for="name">脚本内容：</label>

        <textarea class="form-control" rows="15" style="width: 80%;" v-model="scriptsText" ></textarea>

        <button class="btn x-btn-default-large-bc" style="margin-left: 35%;margin-top: 1.5em" v-on:click="run(event)">运行脚本</button>
        <select  class="form-control" style="width: 10%;z-index:20;margin-left: 35%;margin-top: 1.5em" v-model="selected">
            <option v-for="opt in options" :value="opt">{{opt}}</option>
        </select>
        {{selected}}
    </div>

    <div class="form-group" style="width: 80%;margin-left: 3em">
        <label for="name" >返回数据：</label>
        <textarea class="form-control" rows="15" style="width: 80%;" v-model="scriptsResult"></textarea>
    </div>
</div>
</div>
<div class="col-md-1"></div>

</div>
</body>
</html>